#!/usr/bin/env node
var debug = require('debug')('backend');
var Sequelize = require('sequelize');

var sequelize = new Sequelize('database', 'catdadmom', '1111', {
  dialect: 'sqlite',
  storage: 'database.sqlite'
});


sequelize['sync']()
  .success(function () {
    debug('success db sync');
  }).error(function (error) {
    debug('db sync error : ' + error);
  });
